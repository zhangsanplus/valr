{
  "name": "valr",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@9.15.3",
  "description": "Form Validation Rule Generation",
  "author": "zhangsanplus",
  "license": "MIT",
  "homepage": "https://github.com/zhangsanplus/valr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangsanplus/valr.git"
  },
  "bugs": "https://github.com/zhangsanplus/valr/issues",
  "keywords": [
    "form",
    "rules",
    "validate",
    "validator"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nr -C playground dev",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@antfu/ni": "^23.2.0",
    "@types/node": "^22.10.5",
    "bumpp": "^9.10.0",
    "eslint": "^9.18.0",
    "lint-staged": "^15.3.0",
    "pnpm": "^9.15.3",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.0",
    "vite": "^6.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
