{
  "name": "valr",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.6.5",
  "description": "Form Validation Rule Generation",
  "author": "zhangsanplus",
  "license": "MIT",
  "homepage": "https://github.com/zhangsanplus/valr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangsanplus/valr.git"
  },
  "bugs": "https://github.com/zhangsanplus/valr/issues",
  "keywords": [
    "form",
    "rules",
    "validate",
    "validator"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nr -C playground dev",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/ni": "^24.3.0",
    "@types/node": "^22.13.11",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "pnpm": "^10.6.5",
    "simple-git-hooks": "^2.12.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
